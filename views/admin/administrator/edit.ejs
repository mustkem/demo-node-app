<div id="page-wrapper">
            <div class="container-fluid">
                <div class="row bg-title">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                        <h4 class="page-title">Edit Administrator</h4>
                    </div>
                    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                        <ol class="breadcrumb">
                            <li><a href="/admin/dashboard">Dashboard</a></li>
                            <li class="active">
                                <a href="/admin/administrator">Manage Administrator</a>
                            </li>
                            <li class="active">Edit Administrator</li>
                        </ol>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-info">

                            <div class="panel-wrapper collapse in" aria-expanded="true">
                                <div class="panel-body">
                                    <form action="" method="post" id="registration-form">
                                        <div class="form-body">
                                            <h3 class="box-title">Person Info</h3>
                                            <hr>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="form-group row">
                                                        <label class="control-label col-xs-12 col-md-3">First Name *</label>
                                                        <div class="form-group col-xs-12 col-md-9">
                                                        <input type="text" id="name" name="name" class="form-control" placeholder="Enter Name" data-validation="required" value="<%=usr.fname %>">
                                                          </div>
                                                </div>
                                            </div>
                                        </div>
                                             <div class="row">
                                                <div class="col-md-12">
                                                    <div class="form-group row">
                                                        <label class="control-label col-xs-12 col-md-3">Last Name</label>
                                                        <div class="form-group col-xs-12 col-md-9">
                                                        <input type="text" id="lname" name="lname" class="form-control" placeholder="Enter Last Name" value="<%=usr.lname %>">
                                                        </div>
                                                    </div>                                  </div>
                                            </div>

                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="form-group row">
                                                        <label class="control-label col-xs-12 col-md-3">Email *</label>
                                                        <div class="form-group col-xs-12 col-md-9">
                                                          <input type='hidden' name="new_email" id="new_email" value="<%=usr.email %>">
                                                        <input type="email" class="form-control" name="email" id="email" placeholder="Email Address"  data-validation="email,server" data-validation-url="/admin/administrator/unique_email" data-validation-req-params="<%=JSON.stringify({id:usr._id})%>" value="<%=usr.email %>">
                                                                <span class="form-error" id="form-error-email"></span>
                                                          </div>
                                                </div>
                                            </div>
                                        </div>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="form-group row">
                                                        <label class="control-label col-xs-12 col-md-3">Username *</label>
                                                       <div class="form-group col-xs-12 col-md-9">
                                                         <input type='hidden' name="new_username" id="new_username" value="<%=usr.username %>">
                                                        <input type="text" id="username" name="username" class="form-control" placeholder="Enter Username"  data-validation="required,server" data-validation-url="/admin/administrator/unique_user" data-validation-req-params="<%=JSON.stringify({id:usr._id})%>" value="<%=usr.user %>" >
                                                                <span class="form-error" id="form-error-username"></span>
                                                          </div>
                                                </div>
                                            </div>
                                        </div>

                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="form-group row">
                                                        <label class="control-label col-xs-12 col-md-3">Password *</label>
                                                       <div class="form-group col-xs-12 col-md-9">
                                                        <input type="password" name="password_confirmation" id="password_confirmation" class="form-control" placeholder="Password" >
                                                          </div>
                                                    </div>                                   </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="form-group row">
                                                        <label class="control-label col-xs-12 col-md-3">Confirm Password *</label>
                                                        <div class="form-group col-xs-12 col-md-9">
                                                        <input type="password" name="password" id="password" class="form-control" placeholder="ConfirmPassword" data-validation="confirmation" >
                                                          </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--/row-->
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="form-group row">
                                                        <label class="control-label col-xs-12 col-md-3">Select Role</label>
                                                        <div class="form-group col-xs-12 col-md-9">
                                                        <select name="role_id" id="role_id" class="form-control" data-validation="required">
                                                            <option value="">Select Role</option>
                                                            <%for(var key=0;key<role.length;key++){%>
                                                             <option value="<%=role[key].id%>" <%if(role[key].id ==usr.role){%>selected="seleted"<%}%>>
                                                                <%=role[key].role_title%>
                                                             </option>

                                                            <%}%>
                                                        </select>
                                                          </div>
                                                </div>
                                            </div>
                                        </div>
                                            <hr>


                                        </div>
                                        <div class="form-actions">
                                            <button type="submit" class="btn btn-success btn-form-submit"> <i class="fa fa-check"></i> Save</button>
                                           <a href="/admin/administrator">
                                            <button type="button" class="btn btn-default">Cancel</button>
                                            </a>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- /.container-fluid -->
         </div>
